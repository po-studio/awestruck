pcm.rawjack {
    type jack
     playback_ports {
        0 awestruck:in_jackaudiosrc0_1
        1 awestruck:in_jackaudiosrc0_2
     }

    capture_ports {
        0 jackin:left
        1 jackin:right
     }
}
pcm.jack {
    type plug
    slave { pcm "rawjack" }
    hint {
        description "JACK Audio Connection Kit"
    }
}
pcm.!default {
    type plug
    slave { pcm "rawjack" }
}
